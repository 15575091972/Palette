<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.outstandingteam.palette.mapper.FileMapper">

    <select id="getFilePathById" parameterType="long" resultType="string">
        SELECT file_path
        FROM `file`
        WHERE file_id = #{fileId}
    </select>
</mapper>
